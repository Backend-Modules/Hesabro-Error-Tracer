{
    "name": "hesabro/error-log",
    "version" : "1.0.0",
    "description": "Hesabro error log module",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Moslem Tayari",
            "email": "avapardazmota@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-mongodb": "*",
        "yiisoft/yii2": "~2.0.14",
        "hesabro/helpers": "^1.0",
        "yiisoft/yii2-mongodb": "^2.1",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "kartik-v/yii2-widget-select2": "@dev"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.4"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "hesabro\\errorlog\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "git@git.abrsa.ir:hesabro/backend-modules/hesabro-helpers.git"
        }
    ],
    "extra": {
        "bootstrap": "hesabro\\errorlog\\Bootstrap"
    }
}
